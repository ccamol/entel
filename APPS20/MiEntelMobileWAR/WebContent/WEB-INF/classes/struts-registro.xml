			<result type="freemarker" name="successII">/pages/registroII.html</result>
			<result type="freemarker" name="successIII">/pages/registroIII.html</result>
			<result type="freemarker" name="input">/pages/registro_error.html</result>
			
			<result type="redirectAction" name="trafico">
				<param name="actionName">trafico</param>
			</result>
			
			<result type="redirectAction" name="login">
				<param name="actionName">login</param>
