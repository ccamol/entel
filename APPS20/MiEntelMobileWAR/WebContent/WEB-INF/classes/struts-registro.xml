			<result type="freemarker" name="success_step_medium_one">/pages/medium/registro-paso1.html</result>

			<result type="freemarker" name="success_step_two">/pages/registro-paso2.html</result>
			<result type="freemarker" name="success_step_medium_two">/pages/medium/registro-paso2.html</result>

			<result type="freemarker" name="success_step_three">/pages/registro-paso3.html</result>
			<result type="freemarker" name="success_step_medium_three">/pages/medium/registro-paso3.html</result>


			<result type="freemarker" name="app_success_step_one">/pages/app/registro-paso1.html</result>
			<result type="freemarker" name="app_success_step_two">/pages/app/registro-paso2.html</result>
			<result type="freemarker" name="app_success_step_three">/pages/app/registro-paso3.html</result>
			
