/*-----------------------------------------------------------
		       		   Tablas formularios
-----------------------------------------------------------*/
.tabla_formulario{
	display: block;
	margin: 5px 0;
	padding: 10px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 1.2em;
	border-top: 1px solid #ebebeb;
	border-bottom: 1px solid #ebebeb;
}
.tabla_formulario_fila{
	display: block;
	margin: 10px 0;
	position: relative;
}
.tabla_formulario_label{
	display: block;
	float: left;
	width: 245px;
	padding: 4px 0 0 0;
}
.recortador{
	width: 215px;
}
.tabla_formulario_numero{
	display: block;
	float: left;
	width: 160px;
	padding: 4px 0 0 0;
}
.tabla_formulario_dato{
	display: block;
	float: left;
	margin: 0 10px 0 0;
}
.tabla_formulario_input {
	float:left;
	display:block;
}
/*-----------------------------------------------------------
		       	   			Botones
-----------------------------------------------------------*/
.btnCancelar{
	padding: 0 0 0 10px;
	color: #3a3a3a;
	font-size: 1.2em;
	font-weight: bolder;
}
