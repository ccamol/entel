/*---------------------------------
		   Tabla Historico Recargas
----------------------------------*/
div.header_tabla_historico div.top {
	background:url(../../img/table/bg_bigTablaTop.gif) right top no-repeat;
	height:4px;
	margin-bottom:-3px;
	position:relative;
	font-size:1px;
	padding-right:4px;
	display:block;
}
div.header_tabla_historico div.top span {
	background:url(../../img/table/bg_bigTablaTop.gif) left top no-repeat;
	height:4px;
	display:block;
}
div.header_tabla_historico div.main {
	font-size:1.2em;
	border:1px solid #cddae7;
	border-top:none;
	border-bottom:none;
	background-color:#e3edf8;
    padding: 10px 0 10px 10px;

}

div.header_tabla_historico div.bottom {
	background:url(../../img/table/bg_bigTablaBottom.gif) right bottom no-repeat;
	height:4px;
	margin-top:-3px;
	font-size:1px;
	display:block;
	padding-right:4px;
}
div.header_tabla_historico div.bottom span {
	background:url(../../img/table/bg_bigTablaBottom.gif) left bottom no-repeat;
	height:4px;
	display:block;
}

.imagen_landing_oferta_recarga {
    background: url(../../img/img_landing.jpg) no-repeat scroll right top transparent;
    display: block;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 1.2em;
    padding: 70px 195px 20px 0;
    text-align : justify; 
}

#centro .mensaje_punto_naranja {
    background: url("../../img/bullet_link_naranja.gif") no-repeat scroll left 18px transparent;
    border-bottom: 1px dotted #CCCCCC;
    list-style-type: none;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 1.2em;
    padding: 12px 0 12px 10px;
}