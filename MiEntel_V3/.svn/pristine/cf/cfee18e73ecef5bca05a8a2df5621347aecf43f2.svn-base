/** Tooltip **/
.wrapComponent{		
	
}.wrapComponent div.icoTool{
	display: block;	
	height: 12px;
	cursor: pointer;
	margin: 0;
	padding: 0;
	outline: none;
	width: 12px;
}.wrapComponent div.icoTool span.toolMeta{
	background: none repeat scroll 0 0 #EEEEEE;
    border: 1px solid #DDDDDD;
    color: #646464;
    display: none;
    font-size: 11px;
    height: auto;    
    line-height: 1.5;
    margin-top: 28px;
    padding: 15px;
    position: absolute;
    text-align: left;
    width: 114px;
    z-index: 10;
    margin-left:-40px;
}.wrapComponent div.icoTool:hover span.toolMeta{
	display: block;	
}.wrapComponent div.icoTool span.toolArrowTop{	
	background: url('../../img/tooltips/toolTop.png') no-repeat;
	height: 11px;   
    position: absolute;    
    width: 146px;
    z-index: 11;
    display: none;
    margin-top: 18px;
    margin-left:-40px;
}.wrapComponent div.icoTool:hover span.toolArrowTop{
	display: block;
}.wrapComponent div.icoTool span.toolArrowBottom{	
	background: url('../../img/tooltips/toolBottom.png') no-repeat;
	bottom: 0;
	display: none;
	height: 11px;
	position: absolute;
	width: 146px;
	z-index: 11;
	margin-left:-40px;
}.wrapComponent div.icoTool:hover span.toolArrowBottom{
	display: block;
}

