.linea_tabs {
    color: #064687;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 1.2em;
}

.linea_tabs .tab {
	border-left: 1px solid #CDDAE7;
    border-right: 1px solid #CDDAE7;
    border-top: 1px solid #CDDAE7;
    cursor: pointer;
    display: block;
    float: left;
    margin: 0 2px;
    padding: 4px 6px 5px;
}

.linea_tabs .tab.seleccionado {
    background: none repeat scroll 0 0 #FFFFFF;
    font-weight: bolder;
    border-bottom: 1px solid #FFFFFF;
}

.linea_tabs .tab span {
    padding: 2px 0 2px 22px;
}

.linea_tabs .tab span.informacion {
    background: url("../../img/icons/ico_informacion_small.png") no-repeat scroll left center transparent;
}

.linea_tabs .tab span.velocidades {
    background: url("../../img/icons/ico-reloj.gif") no-repeat scroll left center transparent;
}