#container_migracion{
   display: block;
   width:660px;
   height: 600px;
   background-image: url(../../img/migraciones/banner_home.jpg);
   background-repeat: no-repeat;
   background-position: top left;
   background-color:#fff;
	position: absolute;
   top:16px;
   right: 16px;
}

*+html #container_migracion{
   display: block;
