/**/
.caja-enc-tienda{
    margin: 20px -5px;
    width: 513px;
}
h2.encuentre-ti{
    margin: 0 0 10px;
    font-family: 'Trebuchet MS', Arial, Helvetica, sans-serif;
    font-size: 15px;
    font-weight: normal;
    color: #0154a0;
    line-height: 16px;
    width: 513px;
}
h2.encuentre-ti span{
    color: #ff6702
}
.box-select{
    width: 513px;
    height: 70px;
    background: #F4F4F4;
    padding: 18px 20px 0;
    margin-bottom: 7px;
}
.box-select .box-region{
    float: left;
    margin-right: 19px;
}
.box-select .box-region .ti_region,.box-select .box-comuna .ti_comuna {
    padding-bottom: 0px;    
}
.box-select .box-region .ti_region p, .box-select .box-comuna .ti_comuna p{
    font-family: 'Trebuchet MS', Arial, Helvetica, sans-serif;
    font-size: 12px;
    font-weight: bold;
    color: #636467;
    line-height: 13px;
    
}
.box-select .box-comuna{
    float: left;
    margin-right: 17px;
}
.box-btn-buscar{
    float: left;
    padding-top: 22px;
}


.btnAzulEActivo{
    display: block;
    text-align: center;
    text-decoration: none;
    width: 81px;
    height: 20px;
    background: #FF6700;
    padding-top: 8px;
    font-family: 'Trebuchet MS', Arial, Helvetica, sans-serif;
    font-size: 12px;
    font-weight: bold;
    color: #FFFFFF;
    line-height: 13px;
}


a.btnAzulE{
    display: block;
    text-align: center;
    text-decoration: none;
    width: 81px;
    height: 20px;
    background: #A6A6A6;
    padding-top: 8px;
    font-family: 'Trebuchet MS', Arial, Helvetica, sans-serif;
    font-size: 12px;
    font-weight: bold;
    color: #FFFFFF;
    line-height: 13px;
}

a.btnAzulE span{
	line-height: 13px;
}


/* Link no visitado*/
a.btnAzulEActivo:link {
	color: #FFFFFF;
} 
/*Link visitado*/
a.btnAzulEActivo:visited {
	text-decoration:none; 
	color:#FFFFFF
} 
/*Link activo*/
a.btnAzulEActivo:active {
	text-decoration:none; 
	color:#FFFFFF;
}  

a.btnAzulEActivo:hover {
	background:#FC9B29;
}

.Result_tabla{}
.Result_tabla table.table_result{width: 553px;}
.Result_tabla table.table_result tr.enca{    
    background: #0053A4;
    height: 45px;
}
.Result_tabla table.table_result tr.enca th{
    vertical-align: middle;
    text-align: center;
    font-family: 'Trebuchet MS', Arial, Helvetica, sans-serif;
    font-size: 12px;
    font-weight: bold;
    color: #FFF;
    line-height: 13px;
}
.Result_tabla table.table_result tr{height: 45px;}
.Result_tabla table.table_result tr.impar_b1{background: #EEEEEE;}
.Result_tabla table.table_result tr.lista:hover{background: #CBDDEE;}
.Result_tabla table.table_result tr td{
    text-align: left;
    vertical-align: middle;
    font-family: 'Trebuchet MS', Arial, Helvetica, sans-serif;
    font-size: 11px;
    font-weight: normal;
    color: #636467;
    line-height: 12px;
    padding: 8px 13px;
}
.Result_tabla table.table_result tr td.N_tienda{padding-left: 17px;width: 88px;padding-right: 17px;text-transform: capitalize;}
.Result_tabla table.table_result tr td.D_tienda{padding-left: 15px;padding-right: 7px;width: 98px;}
.Result_tabla table.table_result tr td.HLV_tienda{padding-left: 29px;padding-right: 16px;width: 67px;}
.Result_tabla table.table_result tr td.HS_tienda{padding-left: 10px;width: 51px;padding-right: 6px;}
.Result_tabla table.table_result tr td.HDF_tienda{padding-left: 42px;padding-right: 20px;width: 65px;}

.input{position: relative}
.msj-error{
    position: absolute;
    left: 0px;
    bottom: -42px;
    background: url(../img/globo-error.png) left top no-repeat;
    width: 202px;
    height: 23px;
    text-align: center;
    padding-top: 18px;
}
.msj-error p{
    font-family: 'Trebuchet MS', Arial, Helvetica, sans-serif;
    font-size: 11px !important;
    font-weight: normal;
    color: #FFF;
    line-height: 12px;
}