<?xml version="1.0" ?>
<MBeanType Name="EPCSApplicationAuthenticationProvider" DisplayName="EPCSApplicationAuthenticationProvider"
	Package="com.epcs.seguridad.spi" Extends="weblogic.management.security.authentication.Authenticator">
<MBeanAttribute 
Name = "ProviderClassName" 
Type = "java.lang.String"
Writeable = "false"
Default="&quot;com.epcs.seguridad.spi.EPCSApplicationAuthenticatorImpl&quot;"
Description = "The name of the Java class used to load the Application 
Authentication provider."
/>
<MBeanAttribute 
Name = "Description"
Type = "java.lang.String"
Writeable = "false"
Default = "&quot;Entel Default Application Authentication Provider&quot;"    
Description = "WebService based WebLogic Authentication provider."  />
<MBeanAttribute 
Name = "Version" 
Type = "java.lang.String" 
Writeable = "false" 
Default = "&quot;1.0&quot;" 
Description = "The version of the Application Authentication provider." 
/>
</MBeanType>