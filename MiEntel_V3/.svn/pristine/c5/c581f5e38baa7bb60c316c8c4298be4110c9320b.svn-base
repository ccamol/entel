package com.epcs.bean;

/**
 * Bean que representa la informacion de autenticacion que se requiere de un usuario (numeroPcs,rut,clave) 
 * o una aplicacion (idp).
 * @author mmartinez (I2B) en nombre de Absalon Opazo (Atencion al Cliente, EntelPcs)
 *
 */
public class LoginBean {
    

    private String numeroPcs;
    private RutBean rut;
    private String clave;
    private String idp; 
    /**
     * @return the numeroPcs
     */
    public String getNumeroPcs() {
        return numeroPcs;
    }

    /**
     * @param numeroPcs the numeroPcs to set
     */
    public void setNumeroPcs(String numeroPcs) {
        this.numeroPcs = numeroPcs;
    }

    /**
     * @return the rut
     */
    public RutBean getRut() {
        return rut;
    }

    /**
     * @param rut the rut to set
     */
    public void setRut(RutBean rut) {
        this.rut = rut;
    }

    /**
     * @return the clave
     */
    public String getClave() {
        return clave;
    }

    /**
     * @param clave the clave to set
     */
    public void setClave(String clave) {
        this.clave = clave;
    }

    /**
     * @return the idp
     */
    public String getIdp() {
        return idp;
    }

    /**
     * @param idp the idp to set
     */
    public void setIdp(String idp) {
        this.idp = idp;
    }
}
