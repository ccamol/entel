package com.epcs.bean;

import java.io.Serializable;

/**
 * @author jmanzur (I2B) en nombre de Absalon Opazo (Atencion al Cliente, EntelPcs)
 *
 */
public class PagoAutomaticoBean implements Serializable{

    /**
     * 
     */
    private static final long serialVersionUID = 1L;

    
    private String titular;
    private String rut;
    private String email;
    private String telefonoArea;
    private String telefono;
    private String tipoTarjeta;
    private String numeroTarjeta;
    /**
     * @return the titular
     */
    public String getTitular() {
        return titular;
    }
    /**
     * @param titular the titular to set
     */
    public void setTitular(String titular) {
        this.titular = titular;
    }
    /**
     * @return the rut
     */
    public String getRut() {
        return rut;
    }
    /**
     * @param rut the rut to set
     */
    public void setRut(String rut) {
        this.rut = rut;
    }
    /**
     * @return the email
     */
    public String getEmail() {
        return email;
    }
    /**
     * @param email the email to set
     */
    public void setEmail(String email) {
        this.email = email;
    }
    /**
     * @return the telefonoArea
     */
    public String getTelefonoArea() {
        return telefonoArea;
    }
    /**
     * @param telefonoArea the telefonoArea to set
     */
    public void setTelefonoArea(String telefonoArea) {
        this.telefonoArea = telefonoArea;
    }
    /**
     * @return the telefono
     */
    public String getTelefono() {
        return telefono;
    }
    /**
     * @param telefono the telefono to set
     */
    public void setTelefono(String telefono) {
        this.telefono = telefono;
    }
    /**
     * @return the tipoTarjeta
     */
    public String getTipoTarjeta() {
        return tipoTarjeta;
    }
    /**
     * @param tipoTarjeta the tipoTarjeta to set
     */
    public void setTipoTarjeta(String tipoTarjeta) {
        this.tipoTarjeta = tipoTarjeta;
    }
    /**
     * @return the numeroTarjeta
     */
    public String getNumeroTarjeta() {
        return numeroTarjeta;
    }
    /**
     * @param numeroTarjeta the numeroTarjeta to set
     */
    public void setNumeroTarjeta(String numeroTarjeta) {
        this.numeroTarjeta = numeroTarjeta;
    }
    
}
