@font-face {
    font-family: 'dinnext bold';
    src: url('../fonts/dinnextltpro-bold-webfont.eot');
    src: url('../fonts/dinnextltpro-bold-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/dinnextltpro-bold-webfont.woff') format('woff'),
         url('../fonts/dinnextltpro-bold-webfont.ttf') format('truetype'),
         url('../fonts/dinnextltpro-bold-webfont.svg#dinnext_bold') format('svg');
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'dinnext medium';
    src: url('../fonts/dinnextltpro-medium-webfont.eot');
    src: url('../fonts/dinnextltpro-medium-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/dinnextltpro-medium-webfont.woff') format('woff'),
         url('../fonts/dinnextltpro-medium-webfont.ttf') format('truetype'),
         url('../fonts/dinnextltpro-medium-webfont.svg#dinnext_medium') format('svg');
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'dinnext regular';
    src: url('../fonts/dinnextltpro-regular-webfont.eot');
    src: url('../fonts/dinnextltpro-regular-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/dinnextltpro-regular-webfont.woff') format('woff'),
         url('../fonts/dinnextltpro-regular-webfont.ttf') format('truetype'),
         url('../fonts/dinnextltpro-regular-webfont.svg#dinnext_regular') format('svg');
    font-weight: normal;
    font-style: normal;

}
.fancy_bg{
	background-image: none !important;
}

div#fancy_close  {
	background: url(../img/fancyBox/btCerrar.png) no-repeat left top;
	width: 86px;
	height: 35px;
	top: -35px;
	right: 0;
}

.fbOculto{
	display:none;	
}

.fancyboxCustom{
	padding: 0;
}

.btFcNoVoverMostrar{
	position:absolute;
	display:block;
	background:url(../img/fancyBox/btNoMostrar.png) no-repeat left top;
	width:214px;
	height:35px;
	top:-35px;
	right:100px;
}

.fcCenter {
	position:relative;
	background: url(../img/fancyBox/bgNuevoPlanMultimedia.jpg) no-repeat left top;
	width:880px;
	height:507px;
}
h4.fcTitulo{
	display: block;
	font-family: 'dinnext bold', Arial, Helvetica, sans-serif;
	font-size: 30px;
	line-height: 32px;
	color: #0154A0;
	padding-top:20px;
	margin-bottom:20px;
}.fcTitulo span{
	color: #FF6702;
}
.fcIzq{
	float:left;
	width:420px;
	margin-left:30px;
}
.fcItemInfo{
	display:block;
	font-family: 'Trebuchet MS', Arial, Helvetica, sans-serif;
	font-size: 12px;
	line-height: 14px;
	color: #0154A0;
	margin-bottom:15px;s
}
.fcIcoIM{
	background:url(../img/icons/icoInternetMovil.png) no-repeat left top;
	padding:5px 0 3px 40px;
}
.fcIcoMC{
	background:url(../img/icons/icoMinutosContactos.png) no-repeat left top;
	padding:6px 0 3px 40px;	
}
.fcIcoMT{
	background:url(../img/icons/icoMensajeTextos.png) no-repeat left top;
	padding:6px 0 3px 40px;
}
.fcNota{
	background:url(../img/fancyBox/bgNotaNaranja.png) no-repeat left top;
	width:419px;
	height:67px;
	margin-bottom:20px;
}.fcNota span{
	padding:13px 0 0 75px;
	display:block;
	font-family: 'Trebuchet MS', Arial, Helvetica, sans-serif;
	font-size: 18px;
	line-height: 20px;
	color: #FFF;	
}
.fcSubTitulo{
	display: block;
	font-family: 'Trebuchet MS', Arial, Helvetica, sans-serif;
	font-size: 16px;
	line-height: 18px;
	color: #0154A0;
	margin-bottom:15px;
	font-weight:bold;
}
.fcVineta{
	display:block;
	margin-bottom:15px;
}.fcVineta li{
	display:block;
	background:url(../img/icons/icoPuntoAzul.png) no-repeat left 5px;
	padding-left:15px;
	font-family: 'Trebuchet MS', Arial, Helvetica, sans-serif;
	font-size: 12px;
	line-height: 14px;
	color: #444444;
	padding-bottom:8px;
	
}.fcVineta li a, .fcVineta li span{
	color:#0154A0;
}.fcVineta li a:hover{
	text-decoration:none;
}

.fcFondoGris{
	position:absolute;
	display:block;
	background:url(../img/fancyBox/bgFondoGris.png) no-repeat left top;
	width:293px;
	height:177px;
	padding:20px 30px;
	bottom:25px;
	right:20px;
}

a.enlCorchete {
	display:inline-block;
	background: url(../img/icons/icoFlecha.png) no-repeat right 2px;
	padding-right: 20px;
	font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
	font-size: 14px;
	line-height: 16px;
	color: #444444;
	text-decoration: none;
}a.enlCorchete span{
	color:#FF6702;
	font-weight:bold;
}

.fcBloCor{
	display:block;
	margin-bottom:10px;
	*height:20px;
	*margin-bottom:5px;
}