package com.epcs.bean;

import java.util.List;

public class ItemMatrizCanjeBean {
	private String tipo;    
	private String texto;
    private List<ItemDetalleCanjeBean> listDetalles;
    
	/**
	 * @return the tipo
	 */
	public String getTipo() {
		return tipo;
	}
	/**
	 * @param tipo the tipo to set
	 */
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	/**
	 * @return the listDetalles
	 */
	public List<ItemDetalleCanjeBean> getListDetalles() {
		return listDetalles;
	}
	/**
	 * @param hashDetalles the hashDetalles to set
	 */
	public void setListDetalles(List<ItemDetalleCanjeBean> listDetalles) {
		this.listDetalles = listDetalles;
	}
	/**
	 * @return the texto
	 */
	public String getTexto() {
		return texto;
	}
	/**
	 * @param texto the texto to set
	 */
	public void setTexto(String texto) {
		this.texto = texto;
	}
	
	
	
	
}
