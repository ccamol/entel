package com.epcs.bean;

public class DatosUsuarioBloqueoBean {

	private String email;
	
	private String areaTelefono;
	
	private String telefono;
	
	public DatosUsuarioBloqueoBean(){
		
	}

	/**
	 * @return the email
	 */
	public String getEmail() {
		return email;
	}

	/**
	 * @param email the email to set
	 */
	public void setEmail(String email) {
		this.email = email;
	}

	/**
	 * @return the areaTelefono
	 */
	public String getAreaTelefono() {
		return areaTelefono;
	}

	/**
	 * @param areaTelefono the areaTelefono to set
	 */
	public void setAreaTelefono(String areaTelefono) {
		this.areaTelefono = areaTelefono;
	}

	/**
	 * @return the telefono
	 */
	public String getTelefono() {
		return telefono;
	}

	/**
	 * @param telefono the telefono to set
	 */
	public void setTelefono(String telefono) {
		this.telefono = telefono;
	}
	
}
