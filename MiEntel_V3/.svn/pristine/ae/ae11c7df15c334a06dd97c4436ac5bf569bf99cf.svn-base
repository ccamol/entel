/*
 * Estilos para los controles del carrusel
 */
.jcarousel-clip.jcarousel-clip-horizontal {
	overflow:hidden;
}
.jcarousel-control {
/*	float: left;

    padding-left: 5px;
    padding-top: 5px;
    z-index: 29;
	background-color: #0073AE;*/
	    margin-top: 7px;
}
bannerPrincipal_home{
margin: 0 0 10px 15px;
}
#bannerPrincipal_home .destacado_home{
	display: block;
	margin: 0 0 0 -12px;
}* html #bannerPrincipal_home .destacado_home{
	background: url(../img/circulo_azul.png) no-repeat 8px 13px;
}

/*
 * Estilos para los botones controladores del destacado
 */
#bannerPrincipal_home a.enlace_destacado {
	height:25px;
	width:20px;
	/*margin: 5px 5px 0 0;*/
	padding:3 0 3px 0;
	display: block;
	float: left;
	position: relative;
	z-index: 29;
	text-decoration: none;
	color: white;
	margin-left:8px;
	background: url(../img/circulo_azul.png) no-repeat;
	
}* html #bannerPrincipal_home a.enlace_destacado {
	width: 175px !important;
	margin-left:0px !important;
	background: url(../img/circulo_azul.png) no-repeat;
}

#bannerPrincipal_home a.enlace_seleccionado {
	background: url(../img/circulo_blanco.png) no-repeat;
	color: blue;
	padding:0 0 5px 0;
	
}* html #bannerPrincipal_home a.enlace_seleccionado {
	margin: 0px 3px 0px 0px;
	background: url(../img/circulo_blanco.png) no-repeat;
}*:first-child+html #bannerPrincipal_home a.enlace_seleccionado{
	background: url(../img/circulo_blanco.png) no-repeat;
}

.btnNaranjaCuadrado{
	background: #ff6702;
	color: #FFF;
	display: block;
	font:bold 12px "Trebuchet ms", arial;
	height: 23px;
	padding-top: 7px;
	width:125px;
	margin-top: 8px;
	text-align: center;
	text-decoration: none;
	float: right;
	cursor:pointer;
}

.btnNaranjaCuadrado:hover{
	background: #F37F42;
}


