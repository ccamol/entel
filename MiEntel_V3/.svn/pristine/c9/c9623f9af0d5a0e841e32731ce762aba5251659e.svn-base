public class SituacionActualEquipoBean {
	
    private int cuposDisponibles;
    
    private int cantidadLineasContratadas;
    
    private int equiposContratoVigente;
	
    private ArrayList<EstadoRenovacionEquipoBean> estadoRenovacionEquipo;
    
    public SituacionActualEquipoBean(){
    	estadoRenovacionEquipo = new ArrayList<EstadoRenovacionEquipoBean>(); 
    }
    
	public int getCuposDisponibles() {
		return cuposDisponibles;
	}

	public void setCuposDisponibles(int cuposDisponibles) {
		this.cuposDisponibles = cuposDisponibles;
	}

	public int getCantidadLineasContratadas() {
		return cantidadLineasContratadas;
	}

	public void setCantidadLineasContratadas(int cantidadLineasContratadas) {
		this.cantidadLineasContratadas = cantidadLineasContratadas;
	}

	public int getEquiposContratoVigente() {
		return equiposContratoVigente;
	}

	public void setEquiposContratoVigente(int equiposContratoVigente) {
		this.equiposContratoVigente = equiposContratoVigente;
	}

	public ArrayList<EstadoRenovacionEquipoBean> getEstadoRenovacionEquipo() {
		return estadoRenovacionEquipo;
	}

	public void setEstadoRenovacionEquipo(
			ArrayList<EstadoRenovacionEquipoBean> estadoRenovacionEquipo) {
		this.estadoRenovacionEquipo = estadoRenovacionEquipo;
	}
    
    public void addEstadoRenovacionEquipo(EstadoRenovacionEquipoBean object){
    	this.estadoRenovacionEquipo.add(object);
    }
    
}
