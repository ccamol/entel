package com.epcs.bean;


public class DefectoEquipoBean {
	
    private String codDefecto;

    private String descDefecto;
    
    public DefectoEquipoBean(){
    	
    }

	/**
	 * @return the codDefecto
	 */
	public String getCodDefecto() {
		return codDefecto;
	}

	/**
	 * @param codDefecto the codDefecto to set
	 */
	public void setCodDefecto(String codDefecto) {
		this.codDefecto = codDefecto;
	}

	/**
	 * @return the descDefecto
	 */
	public String getDescDefecto() {
		return descDefecto;
	}

	/**
	 * @param descDefecto the descDefecto to set
	 */
	public void setDescDefecto(String descDefecto) {
		this.descDefecto = descDefecto;
	}
	
}
