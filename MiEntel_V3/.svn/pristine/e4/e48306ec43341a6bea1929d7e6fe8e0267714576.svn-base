public class OrdenTrabajoVigenteBean {

	private ArrayList<OrdenTrabajoBean> ordenes;
	
	public OrdenTrabajoVigenteBean(){
		ordenes = new ArrayList<OrdenTrabajoBean>();
	}

	/**
	 * @return the ordenes
	 */
	public ArrayList<OrdenTrabajoBean> getOrdenes() {
		return ordenes;
	}

	/**
	 * @param ordenes the ordenes to set
	 */
	public void setOrdenes(ArrayList<OrdenTrabajoBean> ordenes) {
		this.ordenes = ordenes;
	}
	
	public void addOrden(OrdenTrabajoBean orden){
		ordenes.add(orden);
	}
	
}
