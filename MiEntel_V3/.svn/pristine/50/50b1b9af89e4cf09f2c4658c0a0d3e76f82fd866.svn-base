package com.epcs.bean;


public class ReparacionEquipoBean {

    private String codReparacion;

    private String descReparacion;
    
    public ReparacionEquipoBean(){
    	
    }

	/**
	 * @return the codReparacion
	 */
	public String getCodReparacion() {
		return codReparacion;
	}

	/**
	 * @param codReparacion the codReparacion to set
	 */
	public void setCodReparacion(String codReparacion) {
		this.codReparacion = codReparacion;
	}

	/**
	 * @return the descReparacion
	 */
	public String getDescReparacion() {
		return descReparacion;
	}

	/**
	 * @param descReparacion the descReparacion to set
	 */
	public void setDescReparacion(String descReparacion) {
		this.descReparacion = descReparacion;
	}
	
}
