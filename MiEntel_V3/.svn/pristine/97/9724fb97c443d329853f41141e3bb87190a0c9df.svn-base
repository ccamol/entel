package com.epcs.bean;

public class ItemProductoCanjeBean implements Comparable<ItemProductoCanjeBean>{
	private String codProducto;    
    private String descProducto;
    private String codBolsa;
    private boolean vigente;
    private int puntos;
    private int monto;       
    private String fechaIniVigencia;
    private String fechaFinVigencia;
    private String nombreUsuario;    
    private String tipoProducto;
    private String letraChica;
    private String codZona;
    private int    anoVigencia;
   
	/**
	 * @return the codProducto
	 */
	public String getCodProducto() {
		return codProducto;
	}
	/**
	 * @param codProducto the codProducto to set
	 */
	public void setCodProducto(String codProducto) {
		this.codProducto = codProducto;
	}
	/**
	 * @return the descProducto
	 */
	public String getDescProducto() {
		return descProducto;
	}
	/**
	 * @param descProducto the descProducto to set
	 */
	public void setDescProducto(String descProducto) {
		this.descProducto = descProducto;
	}
	/**
	 * @return the codBolsa
	 */
	public String getCodBolsa() {
		return codBolsa;
	}
	/**
	 * @param codBolsa the codBolsa to set
	 */
	public void setCodBolsa(String codBolsa) {
		this.codBolsa = codBolsa;
	}
	/**
	 * @return the vigente
	 */
	public boolean isVigente() {
		return vigente;
	}
	/**
	 * @param vigente the vigente to set
	 */
	public void setVigente(boolean vigente) {
		this.vigente = vigente;
	}
	/**
	 * @return the puntos
	 */
	public int getPuntos() {
		return puntos;
	}
	/**
	 * @param puntos the puntos to set
	 */
	public void setPuntos(int puntos) {
		this.puntos = puntos;
	}
	/**
	 * @return the monto
	 */
	public int getMonto() {
		return monto;
	}
	/**
	 * @param monto the monto to set
	 */
	public void setMonto(int monto) {
		this.monto = monto;
	}	
	/**
	 * @return the fechaIniVigencia
	 */
	public String getFechaIniVigencia() {
		return fechaIniVigencia;
	}
	/**
	 * @param fechaIniVigencia the fechaIniVigencia to set
	 */
	public void setFechaIniVigencia(String fechaIniVigencia) {
		this.fechaIniVigencia = fechaIniVigencia;
	}
	/**
	 * @return the fechaFinVigencia
	 */
	public String getFechaFinVigencia() {
		return fechaFinVigencia;
	}
	/**
	 * @param fechaFinVigencia the fechaFinVigencia to set
	 */
	public void setFechaFinVigencia(String fechaFinVigencia) {
		this.fechaFinVigencia = fechaFinVigencia;
	}
	/**
	 * @return the nombreUsuario
	 */
	public String getNombreUsuario() {
		return nombreUsuario;
	}
	/**
	 * @param nombreUsuario the nombreUsuario to set
	 */
	public void setNombreUsuario(String nombreUsuario) {
		this.nombreUsuario = nombreUsuario;
	}	
	/**
	 * @return the tipoProducto
	 */
	public String getTipoProducto() {
		return tipoProducto;
	}
	/**
	 * @param tipoProducto the tipoProducto to set
	 */
	public void setTipoProducto(String tipoProducto) {
		this.tipoProducto = tipoProducto;
	}
	/**
	 * @return the letraChica
	 */
	public String getLetraChica() {
		return letraChica;
	}
	/**
	 * @param letraChica the letraChica to set
	 */
	public void setLetraChica(String letraChica) {
		this.letraChica = letraChica;
	}
	
	/* (non-Javadoc)
	 * @see java.lang.Object#hashCode()
	 */
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result
				+ ((codBolsa == null) ? 0 : codBolsa.hashCode());
		result = prime * result
				+ ((codProducto == null) ? 0 : codProducto.hashCode());
		result = prime * result
				+ ((descProducto == null) ? 0 : descProducto.hashCode());
		result = prime
				* result
				+ ((fechaFinVigencia == null) ? 0 : fechaFinVigencia.hashCode());
		result = prime
				* result
				+ ((fechaIniVigencia == null) ? 0 : fechaIniVigencia.hashCode());
		result = prime * result
				+ ((letraChica == null) ? 0 : letraChica.hashCode());
		result = prime * result + monto;
		result = prime * result
				+ ((nombreUsuario == null) ? 0 : nombreUsuario.hashCode());
		result = prime * result + puntos;		
		result = prime * result
				+ ((tipoProducto == null) ? 0 : tipoProducto.hashCode());
		result = prime * result + (vigente ? 1231 : 1237);
		return result;
	}
	/* (non-Javadoc)
	 * @see java.lang.Object#equals(java.lang.Object)
	 */
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		ItemProductoCanjeBean other = (ItemProductoCanjeBean) obj;
		if (codBolsa == null) {
			if (other.codBolsa != null)
				return false;
		} else if (!codBolsa.equals(other.codBolsa))
			return false;
		if (codProducto == null) {
			if (other.codProducto != null)
				return false;
		} else if (!codProducto.equals(other.codProducto))
			return false;
		if (descProducto == null) {
			if (other.descProducto != null)
				return false;
		} else if (!descProducto.equals(other.descProducto))
			return false;
		if (fechaFinVigencia == null) {
			if (other.fechaFinVigencia != null)
				return false;
		} else if (!fechaFinVigencia.equals(other.fechaFinVigencia))
			return false;
		if (fechaIniVigencia == null) {
			if (other.fechaIniVigencia != null)
				return false;
		} else if (!fechaIniVigencia.equals(other.fechaIniVigencia))
			return false;
		if (letraChica == null) {
			if (other.letraChica != null)
				return false;
		} else if (!letraChica.equals(other.letraChica))
			return false;
		if (monto != other.monto)
			return false;
		if (nombreUsuario == null) {
			if (other.nombreUsuario != null)
				return false;
		} else if (!nombreUsuario.equals(other.nombreUsuario))
			return false;
		if (puntos != other.puntos)
			return false;		
		if (tipoProducto == null) {
			if (other.tipoProducto != null)
				return false;
		} else if (!tipoProducto.equals(other.tipoProducto))
			return false;
		if (vigente != other.vigente)
			return false;
		return true;
	}
	@Override
	public int compareTo(ItemProductoCanjeBean obj) {		
		if(obj != null){
			if(obj.getMonto() > monto){
				return 1;
			}
			else if(obj.getMonto() < monto){
				return -1;
			}
			else{
				return 0;
			}
			
		}
		else{
			return -1;
		}
	}
	/**
	 * @return the codZona
	 */
	public String getCodZona() {
		return codZona;
	}
	/**
	 * @param codZona the codZona to set
	 */
	public void setCodZona(String codZona) {
		this.codZona = codZona;
	}
	/**
	 * @return the anoVigencia
	 */
	public int getAnoVigencia() {
		return anoVigencia;
	}
	/**
	 * @param anoVigencia the anoVigencia to set
	 */
	public void setAnoVigencia(int anoVigencia) {
		this.anoVigencia = anoVigencia;
	}
	
	
	
    
}
