## mientel-web-app build file properties

# especificaiciones BEA y Weblogic
# nombre dominio Weblogic
# EJ: weblogic.domain.name=mydomain
weblogic.domain.name=
# directorio home de BEA
# EJs: bea.home=/bea_10 
#      bea.home=C:\\bea_10
bea.home=

# Directorio base
base=.
# directorios
dir.src=src
dir.dist=dist
dir.web=WebContent
dir.web-inf=${dir.web}/WEB-INF
dir.meta-inf=${dir.web}/META-INF
dir.lib=${dir.web-inf}/lib
dir.build=build/classes
dir.temp=${dir.build}/temp
#javadoc
dir.doc=doc
javadoc.access=private
javadoc.author=true
javadoc.use=true
javadoc.source=1.6
javadoc.version=true

# archivos
dist.filename.war=mientel-web-app.war

# rutas de classpath

# jrockit
jrockit.jre.path=${bea.home}/jrockit_160_14_R27.6.5-32/jre
# Weblogic server home dir
weblogic.server.home.path=${bea.home}/wlserver_10.3
# Weblogic portal home dir
weblogic.portal.home.path=${bea.home}/wlportal_10.3


# Manifest attributes
manifest.vendor=I2B Technologies
