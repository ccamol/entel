***********************************************************
**********************************************************/

.mensaje-error{
	display: block;
	margin:auto;
	padding: 5px 0 5px 0px;
	padding: 5px 0;
	color:#F93849;
	text-align: center;
}

.mensaje-error .contenedor-imagen{
	display:block;
}

.mensaje-error .imagen{
	background:url(../../img/icons/icoAlertaRojo.gif) no-repeat;
	width:23px;
	height:23px;
	float:left;
	margin-left:10px;
}

.mensaje-error .texto{
	font-size:1.2em;
	display: block;
	float:left;
	text-align:left;
	padding: 5px 10px 0 10px;
}

