/**********************************************************
***********************************************************
**********************************************************/
.mensaje-alerta{
	background: #FEFBAD url(../../img/icons/ico_alerta.gif) no-repeat 13px 8px;
	border: 1px solid #F3D2B3;
	border-width: 1px 0;
	margin: 0 0 17px;
	font-size: 1.2em;
	font-weight: bold;
	display: block;
	padding: 8px 0 8px 39px;
	color: #E24D0D;
	border-top: 1px solid #d1c289;
	border-bottom: 1px solid #d1c289;
	margin-bottom:10px;
}

.mensaje-alerta-sistema{
	display: block;
	background: #FFF1A8;
	padding: 10px 0;
	color:#000;
	text-align: center;
	border-top: 1px solid #d1c289;
	border-bottom: 1px solid #d1c289;
	margin-bottom:10px;
}

.mensaje-alerta-sistema .sub-contenedor{
	width:95%;
	margin:0 auto;
}

.mensaje-alerta-sistema .sub-contenedor .contenedor-imagen{
	width:30%;
	float:left;
}

.mensaje-alerta-sistema .imagen{
	background:url(../../img/icons/ico_alerta_amarrillo.gif) no-repeat;
	width:33px;
	height:28px;
	float:right;
	margin:0 auto;
}

.mensaje-alerta-sistema .texto{
	font-size:1.2em;
	width:50%;
	display: block;
	float:left;
	text-align:left;
	padding: 8px 10px 0 10px;
}


/**********************************************************
***********************************************************
**********************************************************/

.mensaje-exito{
	display: block;
	background: #DDF0D9;
	padding: 10px 0;
	color:#000;
	text-align: center;
	border-top: 1px solid #BED7BC;
	border-bottom: 1px solid #BED7BC;
	margin-bottom:20px;
}

.mensaje-exito .sub-contenedor{
	width:95%;
	margin:0 auto;
}

.mensaje-exito .sub-contenedor .contenedor-imagen{
	width:25%;
	float:left;
	padding-top:3px;
}

.mensaje-exito .imagen{
	background:url(../../img/icons/ico_tic_verde.gif) no-repeat;
	float: right;
    height: 15px;
    margin: 0 auto;
    width: 17px;
}

.mensaje-exito .texto{
	font-size:1.2em;
	width:100%;
	display: block;
	text-align:center;
	padding: 5px 0px;;
}

.mensaje-exito .texto2{
	display: block;
    float: left;
    font-size: 1.2em;
    padding: 4px 10px;
    text-align: left;
    width: 65%;
}

.mensaje-exito .texto.cambiarUsuario{
    float: left;
    font-size: 1.2em;
    padding: 10px 0 5px 0;
    text-align: left;
    width: 350px;
}

