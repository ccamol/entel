/*********************************************/
/******* Mensajes JSF con layout TABLE *******/
/*********************************************/

/* IE Hack */
/* Mensajes informacion (exito) */
.contenedor-mensajes-ie  .mensaje-informacion{
	display: block;
	margin:10px auto;
}

.contenedor-mensajes-ie  .mensaje-informacion td{
	width:80%;
	height:40px;
	color:#000000;
	text-align: center;
	border-top: 1px solid #BED7BC;
	border-bottom: 1px solid #BED7BC;
	margin:10px auto;
	font-size:1.2em;
	font-weight:bold;
	display: block;
	padding: 5px 0px;
	background: #DDF0D9;
}

/*********************************************/
/* Mensajes error  */
.contenedor-mensajes-ie  .mensaje-error {
	display: block;
	margin:10px auto;
}

.contenedor-mensajes-ie .mensaje-error td{
	width:80%;
	height:40px;
	color:#000000;
	text-align: center;
	border-top: 1px solid #d1c289;
	border-bottom: 1px solid #d1c289;
	margin:10px auto;
	font-size:1.2em;
	font-weight:bold;
	display: block;
	padding: 5px 0px;
	background:url(../../img/icons/icoAlertaAmarrillo.gif) 50px 15px #FFF1A8 no-repeat;
}

.contenedor-mensajes-ie  .mensaje-error.noIcon td{ background:#FFF1A8;}

/*********************************************/


/* Other browsers */
/*********************************************/
/* Mensajes informacion (exito) */

.contenedor-mensajes .mensaje-informacion {
	display: block;
	margin:10px auto;
	background: #DDF0D9;
	padding: 10px 0;
	color:#000;
	text-align: center;
	border-top: 1px solid #BED7BC;
	border-bottom: 1px solid #BED7BC;	
}

.contenedor-mensajes .mensaje-informacion td {
	width:80%;
	margin:0 auto;
	font-size:1.2em;
	display: block;
	text-align:center;
	padding: 5px 0px;
}

.contenedor-mensajes .mensaje-informacion.noIcon td { background:none; }

/*********************************************/
/* Mensajes error  */

.contenedor-mensajes .mensaje-error {
	
	display: block;
	margin:10px auto;
	padding: 4px 0;
	width: 100%;
	color:#000000;
	text-align: center;
	background: #FFF1A8;
	border-top: 1px solid #d1c289;
	border-bottom: 1px solid #d1c289;
}

.contenedor-mensajes .mensaje-error td {
	width:80%;
	font-weight:bold;
	margin:0 auto;
	font-size:12px;
	display: block;
	text-align:center;
	padding: 5px 5px 5px 25px;
	background:url(../../img/icons/icoAlertaAmarrillo.gif) no-repeat #FFF1A8;
}

.contenedor-mensajes .mensaje-error.noIcon td { background:none; }

/*********************************************/
/* Mensajes Advertencia */

.mensaje-advertencia {
	display: block;
	margin:10px auto;
	padding: 4px 0;
	color:#000000;
	text-align: center;
	background: #FFF1A8;
	border-top: 1px solid #d1c289;
	border-bottom: 1px solid #d1c289;
}

.mensaje-advertencia td {
	width:80%;
	margin:0 auto;
	font-size:1.2em;
	display: block;
	text-align:center;
	padding: 5px 0px;
	background:url(../../img/icons/ico_alertaSmall.gif) no-repeat;
}

.mensaje-advertencia.noIcon td { background:none; }



/*********************************************/
/******* Mensajes JSF con layout LIST *******/
/*********************************************/

/*********************************************/
/* Mensajes Info (exito) */

.mensajes-lista-info {
	display: block;
	width:100%;
	margin:10px auto;
	background: #DDF0D9;
	padding: 10px 0;
	color:#000;
	text-align: center;
	border-top: 1px solid #BED7BC;
	border-bottom: 1px solid #BED7BC;	
}

.mensajes-lista-info.noIcon { background:none; }

.mensajes-lista-info li {	
	width:80%;
	margin:0 auto;
	font-size:1.2em;
	display: block;
	text-align:center;
	padding: 5px 0px;

}

/*********************************************/
/* Mensajes Error  */

.mensajes-lista-error {
	display: block;
	width:100%;
	margin:10px auto;
	background: #FFF1A8 url(../../img/icons/icoAlertaAmarrillo.gif) 60px 10px no-repeat;
	padding: 10px 0;
	color:#000000;
	text-align: center;
	border-top: 1px solid #d1c289;
	border-bottom: 1px solid #d1c289;
}

.mensajes-lista-error.noIcon { background:none; }

.mensajes-lista-error li {	
	font-weight:bold;
	width:80%;
	margin:0 auto;
	font-size:1.2em;
	display: block;
	text-align:center;
	padding: 5px 0px;
	
}

