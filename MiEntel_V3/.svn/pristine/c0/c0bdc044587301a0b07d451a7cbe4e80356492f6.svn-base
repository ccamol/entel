package com.epcs.bean;

import java.io.Serializable;
import java.util.ArrayList;

/**
 * @author I2B, Haltamiranda.
 *
 */
public class ObtenerEquiposSoporteBean implements Serializable{
	
	private static final long serialVersionUID = 1L;
	
	String codigoRespuesta;	
	String mensajeRespuesta;
	int totalEquipos;	
	ArrayList<EquiposSoporteBean> listaEquiposSoporte;	
	
	/**
	 * @return the codigoRespuesta
	 */
	public String getCodigoRespuesta() {
		return codigoRespuesta;
	}
	/**
	 * @param codigoRespuesta the codigoRespuesta to set
	 */
	public void setCodigoRespuesta(String codigoRespuesta) {
		this.codigoRespuesta = codigoRespuesta;
	}
	/**
	 * @return the mensajeRespuesta
	 */
	public String getMensajeRespuesta() {
		return mensajeRespuesta;
	}
	/**
	 * @param mensajeRespuesta the mensajeRespuesta to set
	 */
	public void setMensajeRespuesta(String mensajeRespuesta) {
		this.mensajeRespuesta = mensajeRespuesta;
	}
	/**
	 * @return the listaEquiposSoporte
	 */
	public ArrayList<EquiposSoporteBean> getListaEquiposSoporte() {
		return listaEquiposSoporte;
	}
	/**
	 * @param listaEquiposSoporte the listaEquiposSoporte to set
	 */
	public void setListaEquiposSoporte(
			ArrayList<EquiposSoporteBean> listaEquiposSoporte) {
		this.listaEquiposSoporte = listaEquiposSoporte;
	}
	/**
	 * @return the totalEquipos
	 */
	public int getTotalEquipos() {
		return totalEquipos;
	}
	/**
	 * @param totalEquipos the totalEquipos to set
	 */
	public void setTotalEquipos(int totalEquipos) {
		this.totalEquipos = totalEquipos;
	}	
}