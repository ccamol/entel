div.traficoAlerta{
	background:url(../../img/trafico/img-alerta.png) no-repeat #fff1a8;
	background-position:20px 22px;
	font-size:12px;
	padding:10px 10px 10px 70px;
	border-bottom:1px solid #cfc489;
	border-top:1px solid #cfc489;
	margin:0 0 20px 0;
}
div.traficoAlerta.pequena{
	background:url(../../img/trafico/img-alerta.png) no-repeat #fff1a8;
	background-position:100px 4px;
	font-size:12px;
	padding:5px 5px 10px 5px;
	margin:0 0 20px 0;
	text-align:center;
}h2.icon_sandwatch{
	background:url(../../img/icons/icon_sandwatch.gif) no-repeat scroll 5px 2px;
}div.traficoAlerta p{
	font-size:12px;	
	padding:5px 0 0 0;
}#menu-desplegable div.trafico-anterior-titulo a,#menu-desplegable div.trafico-anterior-titulo a:hover,#menu-desplegable div.trafico-anterior-titulo a:visited {
