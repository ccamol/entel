/*-------------------------------
	  CAJA MEJORA TU PLAN
-------------------------------*/
.mejora_plan {
	display: block;
	margin: 0 0 25px 0;
	padding: 10px 0 10px 25px;
	background: #f5f5f5;
	border-bottom: 1px solid #ebebeb;
	border-top: 1px solid #ebebeb;
}
.mejora_plan .lista_detalles {
	display: block;
	float: left;
	width: 330px;
}
.mejora_plan .lista_detalles ul {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 1.2em;
}
.mejora_plan .lista_detalles ul li {
	list-style: disc;
	list-style-position: inside;
}
.mejora_plan .boton_mejora {
	display: block;
	padding: 3px 0 0 0;
}

