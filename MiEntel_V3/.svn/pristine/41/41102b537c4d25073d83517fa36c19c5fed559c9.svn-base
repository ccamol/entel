	font-weight:bolder;
	text-decoration:underline;
	
}#menu-desplegable div.trafico-anterior-titulo a.abierto, #menu-desplegable div.trafico-anterior-titulo a.abierto{
	background:url(../../img/icons/icon_close.gif) no-repeat 5px 2px;
}#menu-desplegable div.trafico-anterior-titulo{
	padding:10px 0 10px 3px;
	margin:5px 0 5px 0;	
}div#menu-desplegable div.abierto {
	border-right:3px #cfcdcd solid;
	border-bottom:3px #cfcdcd solid;
	border-top:1px #cfcdcd solid;
	border-left:1px #cfcdcd solid;
	background:white;
	padding-right:5px;
}.lista-trafico-anterior{
	border-top:1px #ebebeb solid;
	margin:10px 5px 10px 5px;
	padding:20px 0 0 0;
}

